# file test
# change nfs address as appropriate

[global]
nfs_url=nfs://db-fs2.vsanpe.vmware.com/dbfs
filesize=4G
create_serialize=0
group_reporting
ioengine=nfs
continue_on_error=all
filename_format=fio-test.$clientuid
overwrite=1
allow_file_create=1
iodepth=1
bs=4K

[file_write]
ramp_time=5s
rw=write
